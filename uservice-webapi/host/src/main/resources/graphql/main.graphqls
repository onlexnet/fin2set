type Query {
  view: [ViewEdge!]!
}


type Mutation {
  newMessage(messages: [MessageInput!]!): Message!
}


type Message {
  text: String!
}

input MessageInput {
  text: String!
  role: Role!
}

enum Role {
  USER,
  ASSISTANT
}

type Subscription {
  view: View!
  ticks: Int!
}

enum View {
  CHAT,
  VIEW1
}

type ViewEdge {
    name: String!
}
